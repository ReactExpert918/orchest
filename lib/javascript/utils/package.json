{
  "private": true,
  "name": "@orchest/lib-utils",
  "version": "1.0.0",
  "author": "Orchest B.V.",
  "license": "AGPL-3.0-only",
  "scripts": {
    "build": "pnpm run es:build",
    "dev": "pnpm run es:watch",
    "es:build": "esbuild src/index.js --bundle --outdir=dist --format=esm --loader:.js=jsx --external:react* --define:process.env.NODE_ENV=\\\"production\\\"",
    "es:watch": "esbuild src/index.js --bundle --outdir=dist --format=esm --loader:.js=jsx --external:react* --define:process.env.NODE_ENV=\\\"development\\\" --sourcemap"
  },
  "devDependencies": {
    "esbuild": "0.8.44",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "main": "dist/index.js",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
