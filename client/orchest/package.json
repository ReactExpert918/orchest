{
  "name": "@orchest/client-orchest",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "watch:es": "esbuild src/main.js --bundle --loader:.js=jsx --outfile=dist/main.bundle.js --define:global=window --define:process.env.NODE_ENV=\\\"development\\\" --sourcemap",
    "build:es": "esbuild src/main.js --bundle --loader:.js=jsx --outfile=dist/main.bundle.js --define:global=window --define:process.env.NODE_ENV=\\\"production\\\" --minify --target=chrome58,firefox57,safari11,edge16",
    "watch:sass": "pnpm run build:sass",
    "build:sass": "sass src/styles/main.scss --load-path=node_modules/ --load-path=../../node_modules/ dist/main.css",
    "watch:public": "pnpm run build:public",
    "build:public": "cp -R public dist",
    "build": "concurrently -n w: npm:build:*",
    "dev": "concurrently -n w: npm:watch:*",
    "// IGNORE SCRIPTS BELOW ME //": "////////////////////////////////////////",
    "build-sass": "pnpm run build:sass",
    "build-development": "pnpm run watch:es",
    "watch": "npm-watch"
  },
  "watch": {
    "build-development": "js/src/**/*.js"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@orchest/lib-mdc": "workspace:*",
    "@orchest/lib-utils": "workspace:*",
    "codemirror": "^5.55.0",
    "cron-validate": "^1.4.2",
    "cronstrue": "^1.108.0",
    "dashify": "^2.0.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "material-components-web": "^10.0.0",
    "moment": "^2.29.1",
    "pascalcase": "^1.0.0",
    "react": "^16.13.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "socket.io-client": "^2.3.0",
    "xterm-addon-fit": "^0.4.0",
    "xterm-for-react": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "6.0.1",
    "esbuild": "0.8.44",
    "npm-watch": "^0.7.0",
    "sass": "^1.32.8"
  }
}
