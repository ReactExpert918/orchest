{
  "private": true,
  "scripts": {
    "build": "pnpm run build --r --stream",
    "format": "syncpack format",
    "lint": "syncpack list-mismatches",
    "pre-commit": "pnpm run lint",
    "preinstall": "npx only-allow pnpm",
    "setup": "npm i -g pnpm@6.0.0"
  },
  "dependencies": {
    "syncpack": "^5.7.11"
  },
  "engines": {
    "pnpm": ">=6"
  },
  "syncpack": {
    "sortFirst": [
      "private",
      "name",
      "description",
      "version",
      "author",
      "license",
      "scripts"
    ]
  }
}
