{
  "include": [
  ],

  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/venv"
  ],

  "ignore": [
  ],

  "venvPath": ".venvs",

  "reportMissingImports": true,
  "reportMissingTypeStubs": false,

  "pythonVersion": "3.8",
  "pythonPlatform": "Linux",

  "executionEnvironments": [
    {
      "root": "services/orchest-ctl",
      "venv": "orchest-ctl",
      "extraPaths": [
        "lib/python/orchest-internals/"
      ]
    },
    {
      "root": "orchest-sdk/python",
      "pythonVersion": "3.6",
      "venv": "orchest-sdk"
    },
    {
      "root": "services/orchest-api/app/",
      "venv": "orchest-api",
      "extraPaths": [
        "lib/python/orchest-internals/"
      ]
    },
    {
      "root": "services/memory-server/app",
      "venv": "memory-server",
      "extraPaths": [
        "lib/python/orchest-internals/"
      ]
    }
  ]
}
